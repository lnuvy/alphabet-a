{
  "presets": [
    [
      "next/babel",
      {
        "preset-react": {
          "runtime": "automatic",
          "importSource": "@emotion/react"
        }
      }
    ]
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./src"],
        "alias": {
          "@styles": "/styles",
          "@pages": "/pages",
          "@components": "./src/components",
          "@typing": "./src/typing",
          "@redux": "./src/redux"
        }
      }
    ],
    ["@emotion/babel-plugin"]
  ]
}
